<?xml version='1.0' encoding='utf-8'?>
<widget defaultlocale="it-IT" id="io.cordova.myapp66788c" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
    <name>EllApp</name>
    <description>Instant Message chat for iOS, Android and Windows Phone, written with Apache Cordova with Visual Studio 2015. (In Development)</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">Luigi Elle Balotta</author>
    <content src="core/login/login.html" />
    <access origin="*" />
    <vs:features />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <vs:toolsetVersion>6.3.1</vs:toolsetVersion>
